{
  "name": "@ytvee-dev/eslint-config-ytdev",
  "version": "1.0.0",
  "description": "The general ESLint config for projects.",
  "main": "eslint.config.mjs",
  "keywords": [
    "eslint",
    "bestpractices"
  ],
  "author": "Tyan Yevgeniy",
  "license": "ISC",
  "packageManager": "yarn@4.9.1",
  "repository": "https://github.com/ytvee-dev/eslint-config-ytdev",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "README.md",
    "package.json",
    "yarn.lock",
    "configs",
    "eslint.config.mjs",
    "prettier.js"
  ],
  "scripts": {
    "build": "echo \"Info: Build is unnecessary\"",
    "lint": "yarn eslint . --ext .js,.mjs,.ts,.tsx --report-unused-disable-directives",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.39.0"
  }
}
